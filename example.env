# Example .env file for Aider
# Copy this to .env in your home directory or project root

# API Keys (never commit these to git)
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Aider settings (with AIDER_ prefix)
AIDER_MODEL=gpt-4
# AIDER_USE_CLAUDE=true
AIDER_DARK_MODE=true
# AIDER_VERBOSE=false
# AIDER_AUTO_COMMIT=true
# AIDER_MESSAGE_PREFIX="[aider] "

# Optional: Editor settings
# AIDER_EDITOR=code
# AIDER_VIM=false

# Optional: Set to true to always confirm prompts
# AIDER_YES_ALWAYS=false

# Optional: Display
# AIDER_MULTILINE=false
# AIDER_NOTIFICATIONS=false 